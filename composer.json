{
  "name": "mero/boleto",
  "description": "Library for register and issue brazilian bank slip.",
  "keywords": [
    "boleto",
    "emitir",
    "registrar"
  ],
  "type": "library",
  "license": "MIT",
  "support": {
    "issues": "https://github.com/merorafael/php-boleto/issues",
    "source": "https://github.com/merorafael/php-boleto"
  },
  "authors": [
    {
      "name": "Rafael Mello",
      "email": "merorafael@gmail.com"
    }
  ],
  "require": {
    "php": "^7.0.0",
    "ext-soap": "*",
    "guzzlehttp/guzzle": "~6.0",
    "twig/twig": "^1.35|^2.4.4"
  },
  "require-dev": {
    "phpunit/phpunit": "^6",
    "satooshi/php-coveralls": "~1.0"
  },
  "autoload": {
    "psr-4": {
      "Mero\\BankSlip\\": "src/Mero/BankSlip"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Mero\\BankSlip\\": "tests/Mero/BankSlip"
    }
  },
  "scripts": {
    "test": [
      "phpunit"
    ]
  }
}